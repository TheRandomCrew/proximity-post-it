{
  "name": "proximity-post-it",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run build && node server.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint src --cache -f table",
    "lint:f": "eslint src --fix",
    "prettier": "prettier src --check",
    "test": "jest",
    "test:c": "jest --coverage --watchAll=false --ci --passWithNoTests",
    "test:cw": "jest --coverage --watchAll",
    "ci": "npm run lint && npm run prettier && npm run test:c"
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.9.7",
    "react": "~18.2.0",
    "react-cool-dimensions": "~3.0.1",
    "react-dom": "~18.2.0",
    "react-redux": "~9.0.4",
    "react-router-dom": "~6.18.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~6.1.5",
    "@testing-library/react": "~14.1.2",
    "@testing-library/user-event": "~14.5.2",
    "@types/jest": "~26.0.24",
    "@types/node": "~20.10.6",
    "@types/react": "~18.2.46",
    "@types/react-dom": "~18.2.18",
    "@types/react-router-dom": "~5.1.7",
    "@types/testing-library__jest-dom": "~6.0.0",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "@vitejs/plugin-react-refresh": "~1.3.6",
    "autoprefixer": "~10.4.16",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-config-standard": "~16.0.2",
    "eslint-plugin-import": "~2.29.0",
    "eslint-plugin-jest": "~27.6.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~5.2.0",
    "eslint-plugin-react": "~7.33.2",
    "husky": "~8.0.3",
    "jest": "~27.0.6",
    "jest-coverage-thresholds-bumper": "~1.1.0",
    "lint-staged": "~15.2.0",
    "postcss": "~8.4.31",
    "prettier": "~3.1.0",
    "rollup-plugin-visualizer": "~5.12.0",
    "tailwindcss": "~3.3.5",
    "ts-jest": "~27.0.3",
    "typescript": "~4.9.5",
    "vite": "~4.4.9",
    "vite-plugin-svgr": "~4.2.0",
    "vite-tsconfig-paths": "~4.2.1"
  },
  "engines": {
    "node": "~14.17.0",
    "npm": "~6.14.13"
  },
  "author": {
    "name": "Israel Antonio Rosales Laguan",
    "email": "israellaguan@gmail.com",
    "gitHub": "https://github.com/Israel-Laguan",
    "linkedIn": "https://www.linkedin.com/in/israellaguan/",
    "hireable": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "npm run test:c",
      "npm run test:tb",
      "git add package.json"
    ],
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Israel-Laguan/proximity-post-it.git"
  },
  "bugs": {
    "url": "https://github.com/Israel-Laguan/proximity-post-it/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "ISC"
}
